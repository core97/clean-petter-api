{
  "name": "pet-ad-adoption-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/app/index.ts",
  "scripts": {
    "build": "rimraf ./build && npx tsc -p tsconfig.json",
    "dev": "cross-env NODE_ENV=development nodemon",
    "start": "cross-env NODE_ENV=production npm run build && node build/index.js",
    "test": "cross-env NODE_ENV=test jest",
    "lint": "eslint ./src --ext ts",
    "lint:fix": "eslint --fix --ext .ts .",
    "prettier": "prettier --check src/**/*.ts --check tests/**/*.test.ts",
    "prettier:fix": "prettier --check src/**/*.ts --check tests/**/*.test.ts --write",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@sentry/node": "^7.46.0",
    "awilix": "^8.0.0",
    "bcrypt": "^5.1.0",
    "cloudinary": "^1.36.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "formidable": "^2.1.1",
    "jsonwebtoken": "^9.0.0",
    "node-fetch": "^3.3.1",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@tsconfig/node18": "^1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/formidable": "^2.0.5",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "semi": true
  },
  "lint-staged": {
    "*.(js|ts)": "npm run lint"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "env": {
      "NODE_ENV": "development",
      "TS_NODE_PROJECT": "./tsconfig.json"
    },
    "execMap": {
      "ts": "node --require ts-node/register/transpile-only --require tsconfig-paths/register"
    },
    "ext": "ts, js"
  }
}
